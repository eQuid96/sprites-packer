{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/*"
            ]
        },
        "clean": {
            "cache": false
        },
        "start": {
            "dependsOn": [
                "build"
            ]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}